{
  "name": "end2endtests",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@bitbucket.org:ecawesomemakers/end2endtests.git",
  "author": "arey <arey@redsauce.net>",
  "license": "MIT",
  "devDependencies": {
    "@cucumber/cucumber": "^10.0.1",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@playwright/test": "^1.43.1",
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "cucumber-html-reporter": "^7.1.1",
    "eslint": "^8.53.0",
    "eslint-plugin-playwright": "^0.18.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "test": "playwright test",
    "test:bdd": "cucumber-js",
    "test:bdd:basic": "cucumber-js features/home/acceso-nologin.feature --require step-definitions/test-basic.js",
    "test:bdd:ts": "cucumber-js --require-module ts-node/register --require step-definitions/ --require support/",
    "test:bdd:smoke": "cucumber-js --profile smoke",
    "test:bdd:regression": "cucumber-js --profile regression",
    "test:bdd:tags": "cucumber-js --tags",
    "test:bdd:parallel": "cucumber-js --parallel 2",
    "test:bdd:debug": "cucumber-js --tags @debug",
    "test:bdd:wip": "cucumber-js --tags @wip",
    "test:cucumber:customer": "npx cucumber-js features/customer/ --require ./register.js --require step-definitions/customer/customer-bdd.ts --require support/hooks-bdd.ts",
    "test:cucumber:all": "npx cucumber-js features/ --require ./register.js --require step-definitions/customer/customer-bdd.ts --require step-definitions/home/home.steps.ts --require step-definitions/login/login.steps.ts --require support/hooks-bdd.ts",
    "test:cucumber": "npm run test:cucumber:all",
    "test:generate-report": "node scripts/generate-cucumber-report.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "date-and-time": "^3.0.3",
    "dotenv": "^16.3.1"
  }
}
